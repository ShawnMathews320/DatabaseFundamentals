-- From https://www.w3schools.com/sql/sql_primarykey.asp
-- PRIMARY KEY on CREATE TABLE

-- Code Start
CREATE TABLE Persons (
  ID int NOT NULL,                -- Check that ID exists
  LastName varchar(255) NOT NULL,
  FirstName varchar(255),
  Age int,
  PRIMARY KEY (ID)
 );
 -- Code end
 
 -- The program creates a PRIMARY KEY on the 'ID' column when the "Persons" table is created
